{  
   "extends":[  
      "airbnb",
      "prettier",
      "prettier/react"
      // "eslint-config-prettier"
   ],
   // Support ES6
   "parser": "babel-eslint",
   // Validate JS & JSX files
   "rules":{  
      "react/jsx-filename-extension":[  
         1,
         {  
            "extensions":[  
               ".js",
               ".jsx"
            ]
         }
      ],
      "react/jsx-max-props-per-line": [0],
      "react/no-multi-comp": [0]
   },
   // Allow for console debut settings & test.
   "env":{  
      "browser":true,
      "node":true,
      "jest": true
   },
   // Allow for absolute imports
   "settings": {
      "import/resolver": {
         "node": {
            "paths": ["src/static/"]
         }
      }
   }
}